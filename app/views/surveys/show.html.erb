<p id="notice"><%= notice %></div>





<div>
  <strong>Author:</strong>
  <%= @survey.author.name %>
</div>

<div>
  <strong>Title:</strong>
  <%= @survey.title %>
</div>

<div>
  <strong>Description:</strong>
  <%= @survey.description %>
</div>

<div>
    <% @survey.each do |survey| %>
      <tr>
        <td><%= survey.question %></td>
        <td><%= survey.answer %></td>
      </tr>
    <% end %>
</div>
