<p id="notice"><%= notice %></div>





<div>
  <strong>Author:</strong>
  <%= @survey.author.name %>
</div>

<div>
  <strong>Title:</strong>
  <%= @survey.title %>
</div>

<div>
  <strong>Description:</strong>
  <%= @survey.description %>
</div>
<br>
<div>
  <% @survey.questions.each do |q| %>
    <tr>
      <td><%= q.question_text %></td>
      <td><%= q.answers.answer %></td>
    </tr>
  <% end %>
</div>
